  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/functions/tsconfig.json"
      },
      "name": "Mocha Current File TS",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--no-timeouts",
        "--colors",
        "${file}",
        "--require",
        "ts-node/register"
      ],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "internalConsoleOptions": "neverOpen",
      "runtimeExecutable": "/Users/trex/.nvm/versions/node/v13.8.0/bin/node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Current File JS",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": ["--no-timeouts", "--colors", "${file}"],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "internalConsoleOptions": "neverOpen",
      "runtimeExecutable": "/Users/trex/.nvm/versions/node/v13.8.0/bin/node"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "restart": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
